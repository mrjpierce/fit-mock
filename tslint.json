{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "curly": true,
    "eofline": true,
    "semicolon": [true, "always"],
    "no-console": [
      false
    ],
    "no-empty-interface": false,
    "max-line-length": [
      false
    ],
    "object-literal-sort-keys": false,
    "quotemark": [true, "double"]
  },
  "jsRules": {
    "curly": true
  },
  "rulesDirectory": []
}